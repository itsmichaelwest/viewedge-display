{
  "name": "viewedge-display",
  "author": "Michael West",
  "description": "A small app to display system information on a Cary Works x KBDFANS ViewEdge display.",
  "version": "0.1.5",
  "private": true,
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "opener": "^1.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "spotify-web-api-node": "^5.0.2",
    "systeminformation": "^5.6.12",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "electron": "^12.0.6",
    "electron-builder": "^22.10.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently --kill-others \"yarn start\" \"yarn electron-start\"",
    "electron-start": "electron .",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder --win -c.extraMetadata.main=build/main.js"
  },
  "build": {
    "appId": "nonExistentID",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "protocols": [
      {
        "name": "viewedgedisplay",
        "schemes": [
          "viewedgedisplay"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
